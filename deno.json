{
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "lint": "deno run -A --node-modules-dir npm:@biomejs/biome check --write src",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/"
  },
  "imports": {
    "html-to-image": "npm:html-to-image@^1.11.13",
    "@biomejs/biome": "npm:@biomejs/biome@1.9.4",
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
    "@vitejs/plugin-vue": "npm:@vitejs/plugin-vue@^5.2.1",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@4.0.9",
    "pinia": "npm:pinia@^3.0.1",
    "tailwindcss": "npm:tailwindcss@4.0.9",
    "typescript": "npm:typescript@5.5.3",
    "vite": "npm:vite@6.1.1",
    "vue": "npm:vue@^3.5.13",
    "vue-router": "npm:vue-router@^4.5.0"
  },
  "nodeModulesDir": "auto"
}
